class Usuario:
    def __init__(self, id, nome, email, senha, telefone, endereco):
        self.id = id
        self.nome = nome
        self.email = email
        self.senha = senha
        self.telefone = telefone
        self.endereco = endereco

    def registrar(self):
        pass

    def login(self, email, senha):
        return self.email == email and self.senha == senha

    def recuperar_senha(self, email):
        pass

    def editar_perfil(self, nome, telefone, endereco):
        self.nome = nome
        self.telefone = telefone
        self.endereco = endereco

